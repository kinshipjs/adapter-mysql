{
    "name": "@kinshipjs/adapter-mysql",
    "version": "0.0.1-development",
    "homepage": "https://github.com/kinshipjs/adapter-mysql",
    "repository": {
        "type": "git",
        "url": "https://github.com/kinshipjs/adapter-mysql"
    },
    "description": "Adapter for Kinship that works for MySQL databases.",
    "type": "module",
    "module": "./dist/index.js",
    "types": "./dist/types/adapter.d.ts",
    "scripts": {
        "clean": "del /S /Q .\\dist",
        "build": "npm run clean && npm run build:source && npm run build:types",
        "build:source": "tsup",
        "build:types": "tsc",
        "test": "node ./tests/test.js",
        "prepack": "npm run build"
    },
    "keywords": [
        "myorm",
        "mysql",
        "typescript",
        "table",
        "left join",
        "query",
        "insert",
        "update",
        "delete"
    ],
    "author": "Travis Zuleger",
    "license": "(MIT OR Apache-2.0)",
    "dependencies": {
        "@kinshipjs/core": "file:../../core",
        "@myorm/myorm": "^0.4.3",
        "mysql2": "^3.2.4"
    },
    "devDependencies": {
        "@types/node": "^20.3.1",
        "dotenv": "^16.0.3",
        "drizzle-orm": "^0.27.2",
        "tsup": "^7.2.0",
        "typescript": "^4.9.4"
    },
    "files": [
        "dist/**/*"
    ],
    "exports": {
        ".": {
            "import": {
                "types": "./dist/types/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    }
}
